{
    "deploy_name": "$DEPLOY_NAME",
    "version": "$JAWS_VERSION",
    "docs_url": "https://jaws-docs.readthedocs.io/en/latest/",
    "install_dir": "$INSTALL_DIR",
    "src_dir": "$SRC_DIR",
    "passwords": {
        "jaws_site_cori_db": "$JAWS_SITE_CORI_DB_PW",
        "jaws_site_cori_rmq": "$JAWS_SITE_CORI_RMQ_PW",
        "jaws_cromwell_cori_db": "$JAWS_CROMWELL_CORI_DB_PW",
